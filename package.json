    "classnames": "^2.2.6",
    "copy-text-to-clipboard": "^2.1.1",
    "formik": "^2.1.3",
    "localforage": "^1.7.3",
    "prismjs": "^1.19.0",
    "react": "^16.12.0",
    "react-color": "^2.18.0",
